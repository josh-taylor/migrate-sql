{
  "name": "josh-taylor/migrate-sql",
  "description": "Display the SQL for a migration in Laravel",
  "keywords": [
    "migration",
    "sql",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.0",
    "illuminate/console": "~5.5.0|~5.6.0|~5.7.0",
    "illuminate/database": "~5.5.0|~5.6.0|~5.7.0",
    "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^3.7",
    "phpunit/phpunit": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "Joshtaylor\\MigrateSql\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Joshtaylor\\MigrateSql\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Joshtaylor\\MigrateSql\\MigrateSqlServiceProvider"
      ]
    }
  }
}
